module "google" {
  source  = "hajowieland/k8s/google"

  enable_google = true
  enable_regional_cluster = false
  gcp_project = "<project-id>"
  gcp_region = "<region>"
  gke_name = "tanzu-gke-lab"
  gke_pool_name = "tanzu-gke-lab-pool"
  gke_nodes = 3
  gke_preemptible = false
  gke_node_type = "n1-standard-4"
}

output "kubeconfig_path_gke" {
  value = module.google.kubeconfig_path_gke
}