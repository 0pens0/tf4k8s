module "gke" {
  source  = "../../../modules/cluster/gke"

  gcp_project = "<project-id>"
  gcp_service_account_credentials = "</path/to/service-account.json>"
  gcp_region = "<region>"
  gke_name = "tanzu-gke-lab"
  gke_pool_name = "tanzu-gke-lab-pool"
  gke_nodes = 3
  gke_preemptible = false
  gke_node_type = "n1-standard-4"
}

output "path_to_kubeconfig" {
  value = module.gke.path_to_kubeconfig
}

output "gke_node_version_deployed" {
  value = module.gke.gke_node_version_deployed
}