module "managed-zone" {
    source = "../../../modules/dns/gcp"

    root_zone_name = "<zone-name>"
    environment_name = "<environment-name>"
    dns_prefix = "<dns-prefix>"
}

output "zone_name" {
    value = module.managed-zone.zone_name
}

output "zone_subdomain" {
    value = module.managed-zone.zone_subdomain
}

provider "google" {
  version = ">=3.21.0"
  project = "<project-id>"
}
